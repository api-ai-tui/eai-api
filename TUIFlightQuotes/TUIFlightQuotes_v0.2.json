{
  "openapi" : "3.0.0",
  "servers" : [ ],
  "info" : {
    "description" : "Returns available TUIFlightQuotes given the search/filer criteria provided.",
    "version" : "0.2",
    "title" : "TUIFlightQuotes API",
    "contact" : {
      "name" : "Product Team Integration",
      "email" : "productteamintegration@tui.nl"
    }
  },
  "paths" : {
    "/v0.2/flights/quotes" : {
      "get" : {
        "summary" : "searches TUIFlightQuotes",
        "operationId" : "searchTUIFlightQuotes",
        "description" : "By passing in the appropriate options, you can search for\navailable TUIFlightQuotes in the system\n",
        "parameters" : [ {
          "in" : "query",
          "name" : "queryType",
          "description" : "type of query made. Options are one-way or retour (retour is default)",
          "schema" : {
            "type" : "string",
            "enum" : [ "one-way", "retour" ]
          },
          "example" : "one-way"
        }, {
          "in" : "query",
          "name" : "originIATAAirportCode",
          "description" : "IATA code of the origin airport (single or multiple; comma delimited)",
          "schema" : {
            "type" : "string"
          },
          "example" : "HER"
        }, {
          "in" : "query",
          "name" : "destinationIATAAirportCode",
          "description" : "IATA code of the destination airport (single or multiple; comma delimited)",
          "schema" : {
            "type" : "string"
          },
          "example" : "POP"
        }, {
          "in" : "query",
          "name" : "originDepartureDatePeriodFrom",
          "description" : "Date range departing from origin airport [From date]",
          "schema" : {
            "type" : "string",
            "format" : "date"
          },
          "example" : "2016-06-11"
        }, {
          "in" : "query",
          "name" : "originDepartureDatePeriodTo",
          "description" : "Date range departing from origin airport [To date]",
          "schema" : {
            "type" : "string",
            "format" : "date"
          },
          "example" : "2019-03-07"
        }, {
          "in" : "query",
          "name" : "originArrivalDatePeriodFrom",
          "description" : "Date range arriving at the origin airport [From date]",
          "schema" : {
            "type" : "string",
            "format" : "date"
          },
          "example" : "2020-04-01"
        }, {
          "in" : "query",
          "name" : "originArrivalDatePeriodTo",
          "description" : "Date range arriving at the origin airport [To date]",
          "schema" : {
            "type" : "string",
            "format" : "date"
          },
          "example" : "2019-11-23"
        }, {
          "in" : "query",
          "name" : "destinationDepartureDatePeriodFrom",
          "description" : "Date range departing from destination airport [From date]",
          "schema" : {
            "type" : "string",
            "format" : "date"
          },
          "example" : "2019-03-14"
        }, {
          "in" : "query",
          "name" : "destinationDepartureDatePeriodTo",
          "description" : "Date range departing from destination airport [To date]",
          "schema" : {
            "type" : "string",
            "format" : "date"
          },
          "example" : "2019-03-14"
        }, {
          "in" : "query",
          "name" : "destinationArrivalDatePeriodFrom",
          "description" : "Date range arriving at the destination airport [From date]",
          "schema" : {
            "type" : "string",
            "format" : "date"
          },
          "example" : "2019-03-14"
        }, {
          "in" : "query",
          "name" : "destinationArrivalDatePeriodTo",
          "description" : "Date range arriving at the destination airport [To date]",
          "schema" : {
            "type" : "string",
            "format" : "date"
          },
          "example" : "2019-03-14"
        }, {
          "in" : "query",
          "name" : "originDepartureTimeRangeFrom",
          "description" : "Time range departing from origin airport [From time]",
          "schema" : {
            "type" : "string",
            "format" : "time"
          },
          "example" : "09:30:00-01:00"
        }, {
          "in" : "query",
          "name" : "originDepartureTimeRangeTo",
          "description" : "Time range departing from origin airport [To time]",
          "schema" : {
            "type" : "string",
            "format" : "time"
          },
          "example" : "09:30:00-01:00"
        }, {
          "in" : "query",
          "name" : "originArrivalTimeRangeFrom",
          "description" : "Time range arriving at the origin airport [From time]",
          "schema" : {
            "type" : "string",
            "format" : "time"
          },
          "example" : "09:30:00-01:00"
        }, {
          "in" : "query",
          "name" : "originArrivalTimeRangeTo",
          "description" : "Time range arriving at the origin airport [To time]",
          "schema" : {
            "type" : "string",
            "format" : "time"
          },
          "example" : "09:30:00-01:00"
        }, {
          "in" : "query",
          "name" : "destinationDepartureTimeRangeFrom",
          "description" : "Time range departing from destination airport [From time]",
          "schema" : {
            "type" : "string",
            "format" : "time"
          },
          "example" : "08:00:00+00:30"
        }, {
          "in" : "query",
          "name" : "destinationDepartureTimeRangeTo",
          "description" : "Time range departing from destination airport [To time]",
          "schema" : {
            "type" : "string",
            "format" : "time"
          },
          "example" : "09:30:00-01:00"
        }, {
          "in" : "query",
          "name" : "destinationArrivalTimeRangeFrom",
          "description" : "Time range arriving at the destination airport [From time]",
          "schema" : {
            "type" : "string",
            "format" : "time"
          },
          "example" : "23:30:00+06:00"
        }, {
          "in" : "query",
          "name" : "destinationArrivalTimeRangeTo",
          "description" : "Time range arriving at the destination airport [To time]",
          "schema" : {
            "type" : "string",
            "format" : "time"
          },
          "example" : "13:55:00Z"
        }, {
          "in" : "query",
          "name" : "daysAtDestination",
          "description" : "Duration in days of the time spent on the destination. Return on the same day means 1 day at destination.",
          "schema" : {
            "type" : "integer",
            "minimum" : 1
          },
          "example" : 8
        }, {
          "in" : "query",
          "name" : "paxBirthDate",
          "description" : "Birthdate of each participant, when not provided will return result for one adult.",
          "schema" : {
            "type" : "array",
            "format" : "date"
          },
          "example" : "1986-11-29"
        }, {
          "in" : "query",
          "name" : "limit",
          "description" : "Maximum number of items in the response",
          "schema" : {
            "type" : "integer",
            "minimum" : 0,
            "maximum" : 1000,
            "default" : 100
          }
        } ],
        "responses" : {
          "204 " : {
            "description" : "No Content (returning an empty response)",
            "content" : {
              "application/xml" : {
                "schema" : {
                  "type" : "object",
                  "xml" : {
                    "name" : "TUIFlightQuotes"
                  }
                }
              }
            },
            "headers" : {
              "X-TUI-EAI-Limit" : {
                "$ref" : "#/components/headers/X-TUI-EAI-Limit"
              },
              "X-TUI-EAI-NextOffsetLink" : {
                "$ref" : "#/components/headers/X-TUI-EAI-NextOffsetLink"
              },
              "X-TUI-EAI-Offset" : {
                "$ref" : "#/components/headers/X-TUI-EAI-Offset"
              },
              "X-TUI-EAI-TotalResults" : {
                "$ref" : "#/components/headers/X-TUI-EAI-TotalResults"
              }
            }
          },
          "200 " : {
            "description" : "OK (search results matching criteria)",
            "content" : {
              "application/xml" : {
                "schema" : {
                  "type" : "array",
                  "$ref" : "#/components/schemas/TUIFlightQuotes"
                }
              }
            },
            "headers" : {
              "X-TUI-EAI-Limit" : {
                "$ref" : "#/components/headers/X-TUI-EAI-Limit"
              },
              "X-TUI-EAI-NextOffsetLink" : {
                "$ref" : "#/components/headers/X-TUI-EAI-NextOffsetLink"
              },
              "X-TUI-EAI-Offset" : {
                "$ref" : "#/components/headers/X-TUI-EAI-Offset"
              },
              "X-TUI-EAI-TotalResults" : {
                "$ref" : "#/components/headers/X-TUI-EAI-TotalResults"
              }
            }
          },
          "400 " : {
            "description" : "Bad Request",
            "content" : {
              "application/xml" : {
                "schema" : {
                  "type" : "object",
                  "xml" : {
                    "name" : "error"
                  },
                  "properties" : {
                    "code" : {
                      "type" : "integer",
                      "default" : 400
                    },
                    "description" : {
                      "type" : "string",
                      "default" : "Bad request"
                    }
                  }
                }
              }
            }
          },
          "401 " : {
            "description" : "Unauthorized",
            "content" : {
              "application/xml" : {
                "schema" : {
                  "type" : "object",
                  "xml" : {
                    "name" : "error"
                  },
                  "properties" : {
                    "code" : {
                      "type" : "integer",
                      "default" : 401
                    },
                    "description" : {
                      "type" : "string",
                      "default" : "Unauthorized"
                    }
                  }
                }
              }
            }
          },
          "501 " : {
            "description" : "Internal Server Error",
            "content" : {
              "application/xml" : {
                "schema" : {
                  "type" : "object",
                  "xml" : {
                    "name" : "error"
                  },
                  "properties" : {
                    "code" : {
                      "type" : "integer",
                      "default" : 501
                    },
                    "description" : {
                      "type" : "string",
                      "default" : "Internal Server Error"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "headers" : {
      "X-TUI-EAI-Limit" : {
        "description" : "Maximum number of responses in list. Equals to the query parameter",
        "schema" : {
          "type" : "integer",
          "default" : 34
        }
      },
      "X-TUI-EAI-NextOffsetLink" : {
        "description" : "Link to the next Offset of response objects",
        "schema" : {
          "type" : "string"
        }
      },
      "X-TUI-EAI-Offset" : {
        "description" : "The current Offset",
        "schema" : {
          "type" : "integer"
        }
      },
      "X-TUI-EAI-TotalResults" : {
        "description" : "Total results",
        "schema" : {
          "type" : "integer"
        }
      }
    },
    "schemas" : {
      "TUIFlightQuotes" : {
        "type" : "array",
        "xml" : {
          "name" : "TUIFlightQuotes",
          "wrapped" : true
        },
        "items" : {
          "$ref" : "#/components/schemas/TUIFlightQuote"
        }
      },
      "TUIFlightQuote" : {
        "type" : "object",
        "xml" : {
          "name" : "TUIFlightQuote"
        },
        "properties" : {
          "flightQuoteID" : {
            "type" : "string",
            "example" : 59402367891438907856
          },
          "flightDirection" : {
            "type" : "string",
            "enum" : [ "out", "return" ],
            "example" : "return"
          },
          "TUIFlight" : {
            "type" : "object",
            "$ref" : "#/components/schemas/TUIFlight"
          },
          "quoteItems" : {
            "type" : "array",
            "$ref" : "#/components/schemas/QuoteItems"
          }
        }
      },
      "QuoteItems" : {
        "type" : "array",
        "xml" : {
          "name" : "QuoteItems",
          "wrapped" : true
        },
        "items" : {
          "$ref" : "#/components/schemas/QuoteItem"
        }
      },
      "QuoteItem" : {
        "type" : "object",
        "xml" : {
          "name" : "QuoteItem"
        },
        "properties" : {
          "paxAssignmentType" : {
            "type" : "string",
            "enum" : [ "Adult", "Child", "Infant" ],
            "example" : "Adult"
          },
          "seatsAvailable" : {
            "type" : "integer",
            "example" : 12
          },
          "componentUnitTypeID" : {
            "type" : "string"
          },
          "flightBasePrice" : {
            "type" : "object",
            "$ref" : "#/components/schemas/Money"
          },
          "paxBirthDates" : {
            "type" : "array",
            "xml" : {
              "name" : "paxBirthDates",
              "wrapped" : true
            },
            "items" : {
              "type" : "object",
              "xml" : {
                "name" : "paxBirthDate"
              },
              "properties" : {
                "birthDate" : {
                  "type" : "string",
                  "format" : "date"
                }
              }
            },
            "example" : [ {
              "birthDate" : "1978-11-19"
            }, {
              "birthDate" : "1979-04-25"
            } ]
          }
        }
      },
      "TUIFlight" : {
        "type" : "object",
        "xml" : {
          "name" : "TUIFlight"
        },
        "properties" : {
          "productID" : {
            "type" : "string",
            "example" : "Tussy-1234567890"
          },
          "scheduleEntryID" : {
            "type" : "string"
          },
          "departureIATAAirportCode" : {
            "type" : "string",
            "example" : "BRU"
          },
          "departureDateTime" : {
            "type" : "string",
            "format" : "date-time",
            "example" : "2020-05-31T15:59+02:00"
          },
          "arrivalIATAAirportCode" : {
            "type" : "string",
            "example" : "POP"
          },
          "arrivalDateTime" : {
            "type" : "string",
            "format" : "date-time",
            "example" : "2020-05-31T21:35-04:00"
          },
          "carrierCode" : {
            "type" : "string",
            "example" : "OR"
          },
          "flightNumber" : {
            "type" : "string",
            "example" : "OR302"
          },
          "numberOfStops" : {
            "type" : "integer",
            "example" : 1
          }
        }
      },
      "Money" : {
        "type" : "object",
        "xml" : {
          "name" : "money"
        },
        "properties" : {
          "moneyvalue" : {
            "type" : "integer",
            "example" : "109,90"
          },
          "currency" : {
            "type" : "string",
            "example" : "EUR"
          }
        }
      }
    }
  }
}